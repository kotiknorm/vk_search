<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.zel.test_vk" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_ab_app"
        android:label="@string/app_name"
        android:theme="@style/Theme.Styled" >
        <activity
            android:name=".Views.PeopleListActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vk.sdk.VKOpenAuthActivity" />
        <activity
            android:name=".ApiCallActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_api_call" >
        </activity>

        <activity
            android:name=".Views.SearchParametersActivity"
            android:screenOrientation="portrait">
        </activity>

    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />



</manifest>
